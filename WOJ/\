#include <cstdio>
#include <cstring>
const int N=233333;
const int M=666666;
const int I=0x73333333;

int n,m,s,l;
int eCnt=1;
int ans=0;
int f[N];
int dep[N];
int fa[N];

struct Edge
{
    int to,nx,c,f;
}e[N];

void edgeAdd(int u,int v,int c)
{
    e[++eCnt].to=v;
    e[eCnt].f=u;
    e[eCnt].c=c;
    e[eCnt].nx=f[u];
    f[u]=eCnt;
}

class Queue
{
private:
    int l,r,a[M];
public:
    void clr()
    {
        l=0;r=1;
    }
    bool empty()
    {
        return (l+1==r);
    }
    void push(int x)
    {
        a[r]=x;
        r=(r+1)%M;
    }
    void pop()
    {
        l=(l+1)%M;
    }
    int front()
    {
        return a[(l+1)%M];
    }
    int first()
    {
        int ans=front();
        pop();
        return ans;
    }
};

bool bfs()
{
    memset(dep,-1,sizeof(dep));
}

int main()
{
    
}


